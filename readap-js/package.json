{
  "name": "readap-js",
  "version": "0.1.0",
  "description": "High-level JavaScript/TypeScript client for OpenDAP data access",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "node --test dist/test/*.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "opendap",
    "dap2",
    "oceanography",
    "scientific-data",
    "javascript",
    "typescript"
  ],
  "author": "Matthew Iannucci <mpiannucci@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpiannucci/readap"
  },
  "bugs": {
    "url": "https://github.com/mpiannucci/readap/issues"
  },
  "homepage": "https://github.com/mpiannucci/readap#readme",
  "dependencies": {
    "readap-wasm": "*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}